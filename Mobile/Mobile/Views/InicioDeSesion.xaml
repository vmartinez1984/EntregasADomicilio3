<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Mobile.Views.InicioDeSesion">
    <StackLayout>
        <Label Text="Correo"/>
        <Entry x:Name="Correo" Keyboard="Email"/>
        <Label Text="Contraseña"/>
        <Entry x:Name="Contrasenia"/>       

        <Button Text="Iniciar sesión" x:Name="BtnIniciarSesion" Clicked="BtnIniciarSesion_Clicked"/>

        <Button Text="Registrarse" x:Name="BtnRegistrarse" Clicked="BtnRegistrarse_Clicked"/>
    </StackLayout>
</ContentPage>